{
  "name": "easy-fe-snippets",
  "displayName": "Easy FE Snippets",
  "description": "一个Snippets插件集成了市面上安装量较大的Vue 3 Snippets、Simple React Snippets，另新增一些常用js、ts语法的snippets",
  "version": "1.0.8",
  "icon": "images/react-logo.png",
  "publisher": "zzailianlian",
  "engines": {
    "vscode": "^1.72.0"
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "publish:package": "vsce publish"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zzall/allSnippets-vscode-plugin.git"
  },
  "keywords": [
    "Easy FE Snippets",
    "easy fe snippets",
    "Easy Snippets",
    "easy snippets",
    "FE Snippets",
    "fe snippets",
    "React",
    "React JS",
    "React Snippets",
    "Vue 2 Snippets",
    "Vue",
    "Vscode Plugin",
    "Snippets",
    "snippets",
    "JS",
    "TS",
    "javascript",
    "typescript",
    "easy",
    "Easy",
    "FE",
    "fe"
  ],
  "contributes": {
    "commands": [],
    "snippets": [
      {
        "language": "javascriptreact",
        "path": "./snippets/common-snippets.json"
      },
      {
        "language": "json",
        "path": "./snippets/common-snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/common-snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/common-snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/common-snippets.json"
      },
      {
        "language": "html",
        "path": "./snippets/common-snippets.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/common-snippets.json"
      },
      {
        "language": "vue",
        "path": "./snippets/common-snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/react/snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/react/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/react/snippets-ts.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/react/snippets-ts.json"
      },
      {
        "language": "html",
        "path": "./snippets/vue/html.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/vue/html.json"
      },
      {
        "language": "html",
        "path": "./snippets/vue/vue.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue/javascript.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue/html.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue/vue.json"
      },
      {
        "language": "jade",
        "path": "./snippets/vue/pug.json"
      }
    ]
  },
  "funding": {
    "type": "individual",
    "url": "https://zzailianlian.github.io/#/fund"
  },
  "devDependencies": {
    "@types/vscode": "^1.72.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "ts-loader": "^9.4.1",
